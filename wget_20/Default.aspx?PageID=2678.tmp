	function tabBlockAccordion() {
		if($(".hp").length) {
			switch(csGetBreakPoint()) {
				case "desktop":  case "768": case "640": case "480":
					if(csTabAccordion) {
						$("#sw-content-container3.ui-hp div.ui-widget-header").unbind("click");
						$("#sw-content-container3.ui-hp div.ui-widget.app.active").each(function() {
							$(this).removeClass("active");
						});
						
						csTabAccordion = 0;
					}
				break;
				case "320":
					if(!csTabAccordion) {
						//DOC make homepage apps collapsible for small mobile devices
						if(!$("#sw-content-container3.ui-hp div.ui-widget.app.active").length){
							$("#sw-content-container3.ui-hp div.ui-widget.app:first").addClass("active");
						}
						
						//DOC accordion header click
						$("#sw-content-container3.ui-hp div.ui-widget-header").click(function(){
							var clickedApp = $(this).parent().parent().attr("id");
							
							$(this).parent().toggleClass("active");
							
							$("html, body").animate({
								scrollTop: $("#" + clickedApp).offset().top
							}, 500, function(){
								$("html, body").stop(true, false);
							});
						});
						
						csTabAccordion = 1;
					}
				break;
			}
		}
	}
	
