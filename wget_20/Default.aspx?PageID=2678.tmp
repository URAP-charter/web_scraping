	function stickyChannelBar() {
		switch(csGetBreakPoint()) {
			case "desktop":
				var gbChannelBarOffset = $("nav").offset().top;
				if ($(window).scrollTop() <= gbChannelBarOffset) {
					if(csChannelBarIsSticky) {
						$("#gb-channel-list-outer").removeClass("sticky");
						$("nav").css("height", "auto");
						
						csChannelBarIsSticky = 0;
					}
				} else {
					$("nav").css("height", $("#gb-channel-list-outer").outerHeight(true) + "px");
					
					if(!csChannelBarIsSticky) {
						$("#gb-channel-list-outer").addClass("sticky");
						
						csChannelBarIsSticky = 1;
					}
				}
			break;
			case "768": case "640": case "480": case "320":
				if(csChannelBarIsSticky == 1) {
					$("#gb-channel-list-outer").removeClass("sticky");
					$("nav").css("height", "auto");
					
					csChannelBarIsSticky = 0;
				}
			break;
		}
	}
	
	function csGetBreakPoint() {
		return window.getComputedStyle(document.querySelector("body"), ":before").getPropertyValue("content").replace(/"|'/g, "");/*"*/
	}
	
